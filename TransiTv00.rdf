@prefix : <https://www.cranfield.ac.uk/transit/202501#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://www.cranfield.ac.uk/transit/202501#> .

<https://www.cranfield.ac.uk/transit/202501> rdf:type owl:Ontology ;
                                              owl:imports <http://ies.data.gov.uk/ontology/ies4> ,
                                                          <https://spec.industrialontologies.org/ontology/202401/core/Core/> ;
                                              <http://purl.org/dc/elements/1.1/license> "http://opensource.org/licenses/MIT"@en ;
                                              <http://purl.org/dc/terms/abstract> "The transit Ontology contains terms and concepts found to be common across transportation domain. The ontology itself utilizes BFO as Top-level ontology and IOF-Core as mid-level ontology.  It imports also some existing terms and properties from IES 4. The purpose of the ontology is to represent the diverse processes and entities involved in transpoartation scenario for consistent semantics, paving the way for further development, analysis and optimization on a higher level."@en ;
                                              <http://purl.org/dc/terms/contributor> "Bernadin Namoano, Cranfield University" ;
                                              <http://purl.org/dc/terms/creator> "Zhengyu Liu, Université de Technologie Tarbes Occitanie Pyrénées - UTTOP" ;
                                              rdfs:label "Tranit Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#minQualifiedCardinality
owl:minQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#qualifiedCardinality
owl:qualifiedCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.obolibrary.org/obo/BFO_0000108
<http://purl.obolibrary.org/obo/BFO_0000108> rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasProcessCharacteristic> .


###  https://www.cranfield.ac.uk/transit/202501#_tempPropertyObjectIES4
:_tempPropertyObjectIES4 rdf:type owl:ObjectProperty ;
                         rdfs:comment "This is a tempory placeholder to highlight the added object properties in the developed ontology"@en .


###  https://www.cranfield.ac.uk/transit/202501#appliesEnergy
:appliesEnergy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
               rdfs:domain <http://ies.data.gov.uk/ontology/ies4#Device> ;
               rdfs:range :Energy .


###  https://www.cranfield.ac.uk/transit/202501#chargesEnergy
:chargesEnergy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :appliesEnergy ;
               rdfs:domain :EnergyCharger ;
               rdfs:range :Energy .


###  https://www.cranfield.ac.uk/transit/202501#departsAtOrigin
:departsAtOrigin rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000015>
                                             :route
                                           )
                             ] ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( <http://ies.data.gov.uk/ontology/ies4#Location>
                                            <http://purl.obolibrary.org/obo/BFO_0000030>
                                          )
                            ] .


###  https://www.cranfield.ac.uk/transit/202501#drivenBy
:drivenBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
          owl:inverseOf :drives .


###  https://www.cranfield.ac.uk/transit/202501#drives
:drives rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
        rdfs:range :HumanDrivenVehicle .


###  https://www.cranfield.ac.uk/transit/202501#endsAtDestination
:endsAtDestination rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000015>
                                               :route
                                             )
                               ] ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( <http://ies.data.gov.uk/ontology/ies4#Location>
                                              <http://purl.obolibrary.org/obo/BFO_0000030>
                                            )
                              ] .


###  https://www.cranfield.ac.uk/transit/202501#hasConsumer
:hasConsumer rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasParticipantsInIndustry .


###  https://www.cranfield.ac.uk/transit/202501#hasEmission
:hasEmission rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasParticipantInEmission ;
             rdfs:range :GreenhouseGasEmission .


###  https://www.cranfield.ac.uk/transit/202501#hasEmitter
:hasEmitter rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasParticipantInEmission ;
            rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> .


###  https://www.cranfield.ac.uk/transit/202501#hasMeasuredCostValueAtSomeTime
:hasMeasuredCostValueAtSomeTime rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasMeasuredValueAtSomeTime> .


###  https://www.cranfield.ac.uk/transit/202501#hasMeasuredDimensionValueAtSomeTime
:hasMeasuredDimensionValueAtSomeTime rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasMeasuredValueAtSomeTime> .


###  https://www.cranfield.ac.uk/transit/202501#hasMeasuredTemporalValueAtSomeTime
:hasMeasuredTemporalValueAtSomeTime rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasMeasuredValueAtSomeTime> ;
                                    rdfs:range <https://spec.industrialontologies.org/ontology/core/Core/TemporalDurationValueExpression> .


###  https://www.cranfield.ac.uk/transit/202501#hasParticipantInEmission
:hasParticipantInEmission rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000057> ;
                          rdfs:domain :EmissionProcess ;
                          rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> .


###  https://www.cranfield.ac.uk/transit/202501#hasParticipantsInIndustry
:hasParticipantsInIndustry rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000057> .


###  https://www.cranfield.ac.uk/transit/202501#hasParticipantsInMovement
:hasParticipantsInMovement rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000057> ;
                           owl:inverseOf :participatesInMovement ;
                           rdfs:domain <http://ies.data.gov.uk/ontology/ies4#Movement> .


###  https://www.cranfield.ac.uk/transit/202501#hasPayLoad
:hasPayLoad rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasParticipantsInMovement ;
            rdfs:domain <http://ies.data.gov.uk/ontology/ies4#Movement> ;
            rdfs:comment "put under is carrier of" .


###  https://www.cranfield.ac.uk/transit/202501#hasProducer
:hasProducer rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasParticipantsInIndustry .


###  https://www.cranfield.ac.uk/transit/202501#hasSubstance
:hasSubstance rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000178> ;
              owl:inverseOf :isSubstanceOf .


###  https://www.cranfield.ac.uk/transit/202501#isSubstanceOf
:isSubstanceOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000176> .


###  https://www.cranfield.ac.uk/transit/202501#isVehicleInMovement
:isVehicleInMovement rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :participatesInMovement ;
                     rdfs:domain <http://ies.data.gov.uk/ontology/ies4#Vehicle> ;
                     rdfs:range <http://ies.data.gov.uk/ontology/ies4#Movement> .


###  https://www.cranfield.ac.uk/transit/202501#operatedByVehicle
:operatedByVehicle rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasParticipantsInMovement ;
                   rdfs:domain <http://ies.data.gov.uk/ontology/ies4#Movement> ;
                   rdfs:range <http://ies.data.gov.uk/ontology/ies4#Vehicle> ;
                   rdfs:comment "put under hasParticipant" .


###  https://www.cranfield.ac.uk/transit/202501#ownedBy
:ownedBy rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :_tempPropertyObjectIES4 .


###  https://www.cranfield.ac.uk/transit/202501#owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( <https://spec.industrialontologies.org/ontology/core/Core/Organization>
                                  <https://spec.industrialontologies.org/ontology/core/Core/Person>
                                )
                  ] ;
      rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> .


###  https://www.cranfield.ac.uk/transit/202501#participatesInMovement
:participatesInMovement rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000056> ;
                        rdfs:range <http://ies.data.gov.uk/ontology/ies4#Movement> .


###  https://www.cranfield.ac.uk/transit/202501#passesBy
:passesBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :_tempPropertyObjectIES4 ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( <http://ies.data.gov.uk/ontology/ies4#Movement>
                                      :route
                                    )
                      ] ;
          rdfs:range <http://purl.obolibrary.org/obo/BFO_0000029> .


###  https://www.cranfield.ac.uk/transit/202501#usesEnergy
:usesEnergy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :appliesEnergy ;
            rdfs:range :Energy .


#################################################################
#    Data properties
#################################################################

###  https://www.cranfield.ac.uk/transit/202501#endsAtTimeInstance
:endsAtTimeInstance rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasSimpleExpressionValue> ;
                    rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000015> ;
                    rdfs:range xsd:dateTimeStamp ;
                    rdfs:comment "This is to provide a shortcut to the IOF time expression. May add equivalent to that later" .


###  https://www.cranfield.ac.uk/transit/202501#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasSimpleExpressionValue> ;
         rdfs:range xsd:string .


###  https://www.cranfield.ac.uk/transit/202501#hasNumericalValue
:hasNumericalValue rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasSimpleExpressionValue> .


###  https://www.cranfield.ac.uk/transit/202501#hasUnit
:hasUnit rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasSimpleExpressionValue> ;
         rdfs:range xsd:string .


###  https://www.cranfield.ac.uk/transit/202501#isBooleanProperty
:isBooleanProperty rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasSimpleExpressionValue> ;
                   rdfs:range xsd:boolean .


###  https://www.cranfield.ac.uk/transit/202501#isCarbonNeutral
:isCarbonNeutral rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :isBooleanProperty ;
                 rdfs:range xsd:boolean .


###  https://www.cranfield.ac.uk/transit/202501#isConducted
:isConducted rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :isBooleanProperty ;
             rdfs:range xsd:boolean .


###  https://www.cranfield.ac.uk/transit/202501#isFinished
:isFinished rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :isBooleanProperty ;
            rdfs:range xsd:boolean .


###  https://www.cranfield.ac.uk/transit/202501#isInUse
:isInUse rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :isBooleanProperty .


###  https://www.cranfield.ac.uk/transit/202501#isOnTime
:isOnTime rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :isBooleanProperty ;
          rdfs:domain <http://ies.data.gov.uk/ontology/ies4#Movement> .


###  https://www.cranfield.ac.uk/transit/202501#isPlanned
:isPlanned rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :isBooleanProperty ;
           rdfs:range xsd:boolean .


###  https://www.cranfield.ac.uk/transit/202501#startsAtTimeInstance
:startsAtTimeInstance rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf <https://spec.industrialontologies.org/ontology/core/Core/hasSimpleExpressionValue> ;
                      rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000015> ;
                      rdfs:range xsd:dateTimeStamp .


#################################################################
#    Classes
#################################################################

###  http://ies.data.gov.uk/ontology/ies4#Airport
<http://ies.data.gov.uk/ontology/ies4#Airport> rdfs:subClassOf :CorrespondencePort ;
                                               owl:disjointWith :HarborPort ;
                                               rdfs:comment "Land-to-Air" ;
                                               <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/naturalLanguageDefinition> "An Airport is a correspondence port where aircraft take off, land, and are serviced. Airports connect land travel and air travel."@en .


###  http://ies.data.gov.uk/ontology/ies4#CarTravel
<http://ies.data.gov.uk/ontology/ies4#CarTravel> owl:equivalentClass [ owl:intersectionOf ( <http://ies.data.gov.uk/ontology/ies4#Transit>
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty :operatedByVehicle ;
                                                                                              owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#RoadVehicle>
                                                                                            ]
                                                                                          ) ;
                                                                       rdf:type owl:Class
                                                                     ] .


###  http://ies.data.gov.uk/ontology/ies4#Flight
<http://ies.data.gov.uk/ontology/ies4#Flight> owl:equivalentClass [ owl:intersectionOf ( <http://ies.data.gov.uk/ontology/ies4#Transit>
                                                                                         [ rdf:type owl:Restriction ;
                                                                                           owl:onProperty :operatedByVehicle ;
                                                                                           owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#Aircraft>
                                                                                         ]
                                                                                       ) ;
                                                                    rdf:type owl:Class
                                                                  ] .


###  http://ies.data.gov.uk/ontology/ies4#Journey
<http://ies.data.gov.uk/ontology/ies4#Journey> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000117> ;
                                                                 owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :TravelLegProcess
                                                               ] .


###  http://ies.data.gov.uk/ontology/ies4#Movement
<http://ies.data.gov.uk/ontology/ies4#Movement> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000199> ;
                                                                  owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000008>
                                                                ] ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :departsAtOrigin ;
                                                                  owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#Location>
                                                                ] ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :endsAtDestination ;
                                                                  owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#Location>
                                                                ] ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasParticipantsInMovement ;
                                                                  owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000040>
                                                                ] .


###  http://ies.data.gov.uk/ontology/ies4#RoadVehicle
<http://ies.data.gov.uk/ontology/ies4#RoadVehicle> rdfs:comment "Better be \"Land Vehicle\"?"@en .


###  http://ies.data.gov.uk/ontology/ies4#Sailing
<http://ies.data.gov.uk/ontology/ies4#Sailing> owl:equivalentClass [ owl:intersectionOf ( <http://ies.data.gov.uk/ontology/ies4#Transit>
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty :operatedByVehicle ;
                                                                                            owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#Ship>
                                                                                          ]
                                                                                        ) ;
                                                                     rdf:type owl:Class
                                                                   ] .


###  http://ies.data.gov.uk/ontology/ies4#TrainTravel
<http://ies.data.gov.uk/ontology/ies4#TrainTravel> owl:equivalentClass [ owl:intersectionOf ( <http://ies.data.gov.uk/ontology/ies4#Transit>
                                                                                              [ rdf:type owl:Restriction ;
                                                                                                owl:onProperty :operatedByVehicle ;
                                                                                                owl:someValuesFrom :HDV_Train
                                                                                              ]
                                                                                            ) ;
                                                                         rdf:type owl:Class
                                                                       ] .


###  http://ies.data.gov.uk/ontology/ies4#Transit
<http://ies.data.gov.uk/ontology/ies4#Transit> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :operatedByVehicle ;
                                                                 owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#Vehicle>
                                                               ] ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000117> ;
                                                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :TravelLegProcess
                                                               ] ;
                                               rdfs:comment "Unlike Journet defined in IES 4, I think a tranit (or journey) can have just one travel leg. Like a list (array) can have just one instance, but it is still type list (array)."@en .


###  http://ies.data.gov.uk/ontology/ies4#Vehicle
<http://ies.data.gov.uk/ontology/ies4#Vehicle> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/prescribedBy> ;
                                                                 owl:someValuesFrom :TransportDesignSpecification
                                                               ] ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :appliesEnergy ;
                                                                 owl:someValuesFrom :Energy
                                                               ] .


###  https://www.cranfield.ac.uk/transit/202501#AircraftIdentifier
:AircraftIdentifier rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :VehicleIdentifier
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/designates> ;
                                                                 owl:someValuesFrom <http://ies.data.gov.uk/ontology/ies4#Aircraft>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :VehicleIdentifier .


###  https://www.cranfield.ac.uk/transit/202501#ArtifactIdentifier
:ArtifactIdentifier rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( <https://spec.industrialontologies.org/ontology/core/Core/Identifier>
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/designates> ;
                                                                 owl:someValuesFrom <https://spec.industrialontologies.org/ontology/core/Core/MaterialArtifact>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf <https://spec.industrialontologies.org/ontology/core/Core/Identifier> .


###  https://www.cranfield.ac.uk/transit/202501#AutonomousVehicle
:AutonomousVehicle rdf:type owl:Class ;
                   rdfs:subClassOf <http://ies.data.gov.uk/ontology/ies4#RoadVehicle> ;
                   owl:disjointWith :HumanDrivenVehicle ;
                   rdfs:label "Autonomous Vehicle"@en ;
                   <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/naturalLanguageDefinition> "A road vehicle operating autonomously without real-time human control or a human driver."@en ;
                   <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/synonym> "AVs" .


###  https://www.cranfield.ac.uk/transit/202501#Byproduct
:Byproduct rdf:type owl:Class ;
           rdfs:subClassOf :MaterialResidual ;
           rdfs:label "Byproduct" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/naturalLanguageDefinition> "A secondary material or substance that is produced during the manufacturing or chemical process, which is not the primary product but may have commercial value or potential reuse."@en .


###  https://www.cranfield.ac.uk/transit/202501#CarbonDioxideEmission
:CarbonDioxideEmission rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :GreenhouseGasEmission
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :hasSubstance ;
                                                                    owl:someValuesFrom :Carbon_Dioxide
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :GreenhouseGasEmission ;
                       rdfs:label "Carbon Dioxide Emission" ;
                       <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/synonym> "Carbon Emission"@en .


###  https://www.cranfield.ac.uk/transit/202501#CarbonFootprint
:CarbonFootprint rdf:type owl:Class ;
                 rdfs:subClassOf :GHG_entity ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/hasValueExpressionAtSomeTime> ;
                                   owl:someValuesFrom <https://spec.industrialontologies.org/ontology/core/Core/ValueExpression>
                                 ] ;
                 rdfs:label "Carbon footprint" ;
                 <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/naturalLanguageDefinition> "The net amount of greenhouse gas emissions associated with an entity, process, or product, expressed in terms of carbon dioxide equivalent (CO₂). It is calculated as the total GHG emissions minus the total GHG removals over a defined period or system boundary." .


###  https://www.cranfield.ac.uk/transit/202501#Carbon_Dioxide
:Carbon_Dioxide rdf:type owl:Class ;
                rdfs:subClassOf :Greenhouse_Gas ;
                owl:disjointWith :Methane ;
                rdfs:label "Carbon Dioxide" ;
                <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/naturalLanguageDefinition> "A colorless, odorless greenhouse gas composed of one carbon atom and two oxygen atoms (CO₂). It is a major product of combustion and respiration, and the primary anthropogenic contributor to global warming" ;
                <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/synonym> "CO2" .


###  https://www.cranfield.ac.uk/transit/202501#ChargingStation
:ChargingStation rdf:type owl:Class ;
                 rdfs:subClassOf :LandStation ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/hasComponentPartAtSomeTime> ;
                                   owl:someValuesFrom :EnergyCharger
                                 ] ;
                 rdfs:label "Charging Station"@en ;
                 <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/naturalLanguageDefinition> "A ChargingStation is a land station dedicated to recharging electric vehicles.."@en .


###  https://www.cranfield.ac.uk/transit/202501#Charging_Point
:Charging_Point rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000030> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://spec.industrialontologies.org/ontology/core/Core/hasComponentPartAtSomeTime> ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :EnergyCharger
                                ] ;
                rdfs:comment """A charging station can have one or multilpe charging point, and a charging point have 1+charger.
Each charging point belongs to a specific energy provider, e.g. tesla, total""" ;
                rdfs:label "Charging Point" ;
